{
    "name": "Cloudteam Model Generator",
    "description": "",
    "homepage": "https://github.com/hieupham0206/model-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Phạm Quang Hiếu",
            "email": "hieu.pham@cloudteam.vn"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "~2.5",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/contracts": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/console": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Cloudteam\\ModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cloudteam\\ModelGenerator\\CodersServiceProvider"
            ],
            "aliases": {
                "BaseCore": "Cloudteam\\ModelGenerator\\Facades\\Coders"
            }
        }
    }
}
